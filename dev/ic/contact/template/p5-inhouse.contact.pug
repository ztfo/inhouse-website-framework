section.p5.ih-contact.fw(
  class='{{classes}}',
  ng-style="{'background-image': 'url(' + listing.primaryPhoto.large + ')'}")
  .container
    .contact-inner.flex-grid
      .flex-12.contact-header
        h1.ih-header.uppercase Get In Touch
        .ih-sm-divider
      .flex-6.contact-form-inner
        ih-contact(configname="f1")
      .flex-6.contact-social
        .social-block
          a.ih-social-icon(href='mailto:{{agent.contactEmail}}', ng-if='agent.contactEmail !== undefined')
            i.fa.fa-envelope
          a {{agent.contactEmail}}
        .social-block
          a.ih-social-icon(itemprop='telephone')
            i.fa.fa-phone
          span {{agent.contactPhone | tel}}
        .social-block
          a.ih-social-icon(href='{{agent.facebook}}', ng-if='agent.facebook !== undefined', target='_blank')
            i.fa.fa-facebook
          span {{agent.facebook}}
        .social-block
          a.ih-social-icon(href='{{agent.linkedin}}', ng-if='agent.linkedin !== undefined', target='_blank')
            i.fa.fa-linkedin
          span {{agent.linkedin}}
        .social-block
          a.ih-social-icon(href='{{agent.twitter}}', ng-if='agent.twitter !== undefined', target='_blank')
            i.fa.fa-twitter
          span {{agent.twitter}}
        .social-block
          a.ih-social-icon(href='{{agent.instagram}}', ng-if='agent.instagram !== undefined', target='_blank')
            i.fa.fa-instagram
          span {{agent.instagram}}
        .social-block
          a.ih-social-icon(href='{{agent.pinterest}}', ng-if='agent.pinterest !== undefined', target='_blank')
            i.fa.fa-pinterest
          span {{agent.pinterest}}
  .footer
    .container
      .col-lg-6.col-md-6.col-sm-6.col-xs-12.text-center-xs
        span.ih-ui-fairhousing
        span.ih-ui-realtor
        small Â© {{year}} - {{agent.agentName}} | {{listing.MLS}}
      .col-lg-6.col-md-6.col-sm-6.col-xs-12.text-center-xs.text-right
        .pull-right
          a(href="https://www.getinhouse.io", target="_blank", rel="noopener")
            span.ih-icon.ih-sm-icon.ih-ui-inhouse-emblem
        .pull-right.text-uppercase
          small
            | inspired by &nbsp;
            a(href="https://html5up.net/", target="_blank", rel="noopener") html5up
          br
          small powered by &nbsp;
            a(href="http://www.getinhouse.io") InHouse
