section.p4.ih-slider.fp.bkc-01(class='{{classes}}')
  .greeting.container-fluid.text-center.fw.z2
    div
      img.img-circle.img-responsive(ng-src='{{agent.team[0].photo}}', alt='{{agent.team[0].name}}')
    h1 {{listing.address}}
    p.lead {{listing.description}}
    p
      small.text-uppercase presented by: {{agent.agentName}}
  #nav.container-fluid.alt
    ul
      li
        a(href="#about") About Listing
      li
        a(href="#amenities") Home Amenities
      li
        a(href="#nearby") Nearby Places
      li
        a(href="#agent") About Agent
      li
        a(href="#testimonials") Testimonials
      li
        a(href="#lender") About Lender
      li
        a(href="#contact") Contact
