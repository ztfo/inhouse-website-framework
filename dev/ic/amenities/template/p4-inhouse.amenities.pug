section.p4.ih-amenities.fw
  .container-fluid.header-inner
    h2.ih-header Home Amenities
    .ih-sm-divider
  .container-fluid.amenities-inner
    .col-lg-3.col-md-3.col-sm-3.col-xs-6.amenity(ng-repeat='amenity in amenities')
