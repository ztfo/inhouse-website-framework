                            small.text-muted {{listing.sewer}}
                    tr
                        th
                            small.text-muted Athletic Facilities:
                        td
                            small.text-muted {{listing.athletic}}
                        th
                            small.text-muted Basement:
                        td
                            small.text-muted {{listing.basement}}
                        th
                            small.text-muted Cable Available:
                        td
                            small.text-muted {{listing.cable}}
                    tr
                        th
                            small.text-muted Dividable Lot:
                        td
                            small.text-muted {{listing.dividable}}
                        th
                            small.text-muted Fix up:
                        td
                            small.text-muted {{listing.fix}}
                        th
                            small.text-muted Paved Street
                        td
                            small.text-muted {{listing.paved}}
                    tr
                        th
                            small.text-muted Horse Facilities
                        td
                            small.text-muted {{listing.horsefacilities}}
                        th
                            small.text-muted Horses Allowed:
                        td
                            small.text-muted {{listing.horseallowed}}
                        th
